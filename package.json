{
  "name": "typechain-demo-app",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "dev": "vite",
    "server:dev": "ts-node-dev server",
    "sc:types": "typechain --target ethers-v5 --out-dir src/contracts 'ABI/*.json'",
    "sc:api-types": "typechain --target ethers-v5 --out-dir server/contracts 'ABI/*.json'",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prepare": "husky install"
  },
  "dependencies": {
    "ethers": "5.7.0",
    "express": "^5.0.0-beta.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@storybook/addon-actions": "6.5.10",
    "@storybook/addon-essentials": "6.5.10",
    "@storybook/addon-interactions": "6.5.10",
    "@storybook/addon-links": "6.5.10",
    "@storybook/react": "6.5.10",
    "@storybook/testing-library": "^0.0.11",
    "@trivago/prettier-plugin-sort-imports": "3.3.0",
    "@typechain/ethers-v5": "10.1.0",
    "@types/express": "4.17.14",
    "@types/react": "^18.0.12",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "@unocss/preset-wind": "0.45.28",
    "@vitejs/plugin-react": "^1.3.2",
    "babel-loader": "^8.2.5",
    "eslint": "8.25.0",
    "eslint-config-productsway": "0.1.16",
    "husky": ">=8",
    "lint-staged": "13.0.3",
    "prettier": "^2.6.2",
    "ts-node": "10.9.1",
    "ts-node-dev": "^2.0.0",
    "typechain": "^8.0.0",
    "typescript": "4.7.4",
    "unocss": "^0.37.4",
    "vite": "2.9.15"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  }
}
