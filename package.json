{
  "name": "typechain-demo-app",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "dev": "vite",
    "server:dev": "ts-node-dev server",
    "sc:types": "typechain --target ethers-v5 --out-dir src/contracts 'ABI/*.json'",
    "sc:api-types": "typechain --target ethers-v5 --out-dir server/contracts 'ABI/*.json'",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prepare": "husky install"
  },
  "dependencies": {
    "ethers": "5.7.2",
    "express": "5.0.1",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@babel/core": "7.26.9",
    "@storybook/addon-actions": "7.6.20",
    "@storybook/addon-essentials": "7.6.20",
    "@storybook/addon-interactions": "7.6.20",
    "@storybook/addon-links": "7.6.20",
    "@storybook/react": "7.6.20",
    "@storybook/testing-library": "0.2.2",
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@typechain/ethers-v5": "10.2.1",
    "@types/express": "4.17.21",
    "@types/react": "18.3.18",
    "@types/react-dom": "18.3.5",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "@unocss/preset-wind": "0.65.4",
    "@vitejs/plugin-react": "3.1.0",
    "babel-loader": "9.2.1",
    "eslint": "8.57.1",
    "eslint-config-productsway": "0.3.0",
    "husky": "8.0.3",
    "lint-staged": "13.3.0",
    "prettier": "2.8.8",
    "ts-node": "10.9.2",
    "ts-node-dev": "2.0.0",
    "typechain": "8.3.2",
    "typescript": "4.9.5",
    "unocss": "0.65.4",
    "vite": "4.5.9"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  }
}
